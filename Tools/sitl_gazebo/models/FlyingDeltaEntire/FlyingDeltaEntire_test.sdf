<sdf version='1.5'>
  <model name='FlyingDeltaEntire_test'>
    <include>
      <uri>model://FlyingDelta</uri>
      <pose>0 0 1.60 0 0 0</pose>
    </include>
    <include>
      <uri>model://FlyingDeltaArmBase</uri>
      <pose>0 0 1.503 0 0 -1.5708</pose>
    </include>
    <include>
      <uri>model://velodyne_VLP_16</uri>
      <pose>0 0 1.66 0 0 0</pose>
    </include>
    <joint name="velodyne_base_mount_joint" type="fixed">
      <parent>FlyingDelta::base_link</parent>
      <child>velodyne_VLP_16::velodyne_base_link</child>
    </joint>
    <joint name="velodyne_base_mount_joint_2" type="fixed">
      <parent>FlyingDelta::base_link</parent>
      <child>velodyne_VLP_16::velodyne</child>
    </joint>

    <include>
      <uri>model://gripper1</uri>
      <pose>-0.0 -0.0 1.18 1.5708 0 0</pose>
    </include>
    <joint name="marker_fixed" type="fixed">
      <parent>DeltaManipulator_low::low_base_link</parent>
      <child>gripper1::base_link</child>
    </joint>

    <include>
      <uri>model://DeltaManipulator_low</uri>
      <pose>0.0 -0.0 1.17 0 0 -1.5708</pose>	  
    </include>
    <joint name='uleg1_con' type='revolute'>
      <child>DeltaManipulator_low::con101</child>
      <parent>FlyingDeltaArmBase::uleg_1</parent>
      <axis>
        <xyz>-1 0 0</xyz>
        <limit>
          <lower>-1</lower>
          <upper>1</upper>
        </limit>
        <dynamics>
          <spring_reference>0</spring_reference>
          <spring_stiffness>0</spring_stiffness>
          <damping>0.00</damping>
	  <friction>0.00</friction>
        </dynamics>
        <use_parent_model_frame>0</use_parent_model_frame>
      </axis>
    </joint>
   <joint name='uleg3_con' type='revolute'>
      <child>DeltaManipulator_low::con201</child>
      <parent>FlyingDeltaArmBase::uleg_3</parent>
      <axis>
        <xyz>1 0 0</xyz>
        <limit>
          <lower>-1</lower>
          <upper>1</upper>
        </limit>
        <dynamics>
          <spring_reference>0</spring_reference>
          <spring_stiffness>0</spring_stiffness>
          <damping>0.00</damping>
	  <friction>0.00</friction>
        </dynamics>
        <use_parent_model_frame>0</use_parent_model_frame>
      </axis>
    </joint>
   <joint name='uleg2_con' type='revolute'>
      <child>DeltaManipulator_low::con301</child>
      <parent>FlyingDeltaArmBase::uleg_2</parent>
      <axis>
        <xyz>1 0 0</xyz>
        <limit>
          <lower>-1</lower>
          <upper>1</upper>
        </limit>
        <dynamics>
          <spring_reference>0</spring_reference>
          <spring_stiffness>0</spring_stiffness>
          <damping>0.00</damping>
	  <friction>0.00</friction>
        </dynamics>
        <use_parent_model_frame>0</use_parent_model_frame>
      </axis>
    </joint>

