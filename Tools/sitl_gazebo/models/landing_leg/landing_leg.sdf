	<!-- ######## velodyne ######### -->
		<!-- Give the base link a unique name -->
		<link name="velodyne">
		<!-- Vertically offset the velodyne cylinder by the length of the bottom
			cylinder and half the length of this cylinder. -->
		<pose>0 0 -0.1 0 0 0</pose>
		<inertial>
			<mass>0.01</mass>
			<inertia>
			<ixx>0.00001</ixx>
			<iyy>0.00001</iyy>
			<izz>0.00001</izz>
			<ixy>0</ixy>
			<ixz>0</ixz>
			<iyz>0</iyz>
			</inertia>
		</inertial>
		<collision name="velodyne_collision">
			<geometry>
			<cylinder>
				<!-- Radius and length provided by Velodyne -->
				<radius>0.02</radius>
				<length>0.02</length>
			</cylinder>
			</geometry>
		</collision>
		<!-- The visual is mostly a copy of the collision -->
		<visual name="velodyne_visual">
			<geometry>
			<cylinder>
				<radius>0.02</radius>
				<length>0.02</length>
			</cylinder>
			</geometry>
        <material>
          <script>
            <name>Gazebo/Green</name>
            <uri>file://media/materials/scripts/gazebo.material</uri>
          </script>
        </material>
		</visual>
		<!-- Add a ray sensor, and give it a name -->
		<sensor type="ray" name="sensor">
      <always_on>true</always_on>
			<!-- Position the ray sensor based on the specification. Also rotate
				it by 90 degrees around the X-axis so that the <horizontal> rays
				become vertical -->
			<pose>0 0 0 0 1.5708 0</pose>
			<!-- Enable visualization to see the rays in the GUI -->
			<visualize>true</visualize>
			<!-- Set the update rate of the sensor -->
			<update_rate>20</update_rate>
			<ray>
				<!-- The scan element contains the horizontal and vertical beams.
					We are leaving out the vertical beams for this tutorial. -->
				<scan>
				<!-- The horizontal beams -->
				<horizontal>
					<!-- The velodyne has 32 beams(samples) -->
					<samples>1</samples>
					<!-- Resolution is multiplied by samples to determine number of
						simulated beams vs interpolated beams. See:
						http://sdformat.org/spec?ver=1.6&elem=sensor#horizontal_resolution
						-->
					<resolution>1</resolution>
					<!-- Minimum angle in radians -->
					<min_angle>0.0001</min_angle>
					<!-- Maximum angle in radians -->
					<max_angle>-0.0001</max_angle>
				</horizontal>
				</scan>
				<!-- Range defines characteristics of an individual beam -->
				<range>
				<!-- Minimum distance of the beam -->
				<min>0.05</min>
				<!-- Maximum distance of the beam -->
				<max>10</max>
				<!-- Linear resolution of the beam -->
				<resolution>0.02</resolution>
				</range>
			</ray>
		</sensor>
	  </link>
		<joint type="revolute" name="velodyne_revolute">
			<parent>base_link</parent>
			<child>velodyne</child>
      <pose>0 0 -0 0 0 0</pose>
      <axis>
        <xyz>0 -1 0</xyz>
